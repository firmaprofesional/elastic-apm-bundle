{
    "name": "firmaprofesional/elastic-apm-bundle",
    "description": "Elastic APM bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Miquel Llagostera",
            "email": "mllagostera@firmaprofesional.com"
        }
    ],
    "keywords": ["elastic", "apm", "symfony bundle", "elastic apm"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "FP\\ElasticApmBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "philkra/elastic-apm-php-agent": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
