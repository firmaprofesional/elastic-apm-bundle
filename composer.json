{
    "name": "firmaprofesional/elastic-apm-bundle",
    "description": "Elastic APM bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Miquel Llagostera",
            "email": "mllagostera@firmaprofesional.com"
        }
    ],
    "keywords": ["elastic", "apm", "symfony bundle", "elastic apm"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "FP\\ElasticApmBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "FP\\ElasticApmBundle\\": "tests/" }
    },
    "require": {
        "php": ">=7.1",
            "nipwaayoni/elastic-apm-php-agent": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
